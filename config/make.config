#Automatically generated by configure
#compilers and linkers

#config name
LABEL=icc

#compilers
OMPC=icc -qopenmp -I${HOME}/inst/argobots/opt/include
CC=icc
OMPLINK=icc -qopenmp -labt -L${HOME}/inst/argobots/opt/lib
CLINK=icc

#compiler and linker flags

ifeq (${MIC}, true)
    ARCH=-xMIC-AVX512
else
    ARCH=-xHOST
endif

ifeq (${DEBUG}, true)
    OPT_FLAGS=${ARCH} -g
else
    OPT_FLAGS=${ARCH} -O3 -ipo
endif

CC_FLAGS=-std=c11
OMPC_FLAGS=-std=c11
OMPC_FINAL_FLAGS=

CLINK_FLAGS=
OMPLINK_FLAGS=
